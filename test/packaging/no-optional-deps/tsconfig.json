{
  "compilerOptions": {
    "target": "es2020",
    "module": "es2020",
    "moduleResolution": "node",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "maxNodeModuleJsDepth": 0
  },
  "include": ["index.ts"],
  "exclude": ["node_modules"]
}
